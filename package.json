{
    "name": "rinse",
    "version": "1.0.0",
    "type": "module",
    "description": "Car wash booking service built with the MERN stack",
    "main": "server.js",
    "scripts": {
        "start": "node backend/server.js",
        "server": "NODE_ENV=development nodemon backend/server.js",
        "client": "npm run dev --prefix frontend",
        "dev": "NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
        "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
        "generate-toc": "markdown-toc -i readme.md",
        "lint": "eslint .",
        "data:import": "node backend/seeder.js",
        "data:destroy": "node backend/seeder.js -D",
        "data:import-test": "NODE_ENV=test node backend/seeder.js",
        "data:destroy-test": "NODE_ENV=test node backend/seeder.js -D",
        "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dancarlton/rinse.git"
    },
    "author": "Dan Carlton",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dancarlton/rinse.git/issues"
    },
    "homepage": "https://github.com/dancarlton/rinse.git",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.2",
        "@sendgrid/mail": "^7.7.0",
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "connect-mongo": "^5.0.0",
        "cookie-parser": "^1.4.6",
        "cookie-session": "^2.0.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.10",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^7.0.1",
        "express-session": "^1.17.3",
        "helmet": "^7.0.0",
        "joi": "^17.10.2",
        "jsonwebtoken": "^9.0.0",
        "moment": "^2.29.4",
        "mongo-sanitize": "^1.1.0",
        "mongoose": "^7.0.1",
        "mongoose-unique-validator": "^4.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-local": "^1.0.0",
        "pino": "^8.15.6",
        "pino-http": "^8.5.0",
        "pino-http-print": "^3.1.0",
        "prettier": "^2.5.1",
        "ramda": "^0.29.0",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.17.0",
        "@flydotio/dockerfile": "^0.4.10",
        "@types/express": "^4.17.17",
        "concurrently": "^7.6.0",
        "eslint": "^8.10.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.4.0",
        "jest": "^27.5.1",
        "jshint": "^2.13.4",
        "markdown-toc": "^1.2.0",
        "nodemon": "^2.0.21",
        "supertest": "^6.3.3"
    },
    "jest": {
        "testEnvironment": "node",
        "globalTeardown": "./backend/tests/teardown.js"
    }
}
